{
    "name": "tilleuls/sorter",
    "description": "A library to dynamically sort collections / queries",
    "type": "library",
    "keywords": ["symfony", "symfony-bundle", "sort", "column-sort", "doctrine"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": ">=5.4,<8"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.9",
        "doctrine/orm": "^2.5|^3.0",
        "friendsofphp/php-cs-fixer": "^3.72",
        "infection/infection": "^0.28.1",
        "phpmyadmin/sql-parser": "^5.11",
        "phpunit/phpunit": "^10.5.40",
        "symfony/browser-kit": ">=5.4,<8",
        "symfony/css-selector": ">=5.4,<8",
        "symfony/framework-bundle": ">=5.4,<8",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": ">=5.4,<8",
        "symfony/twig-bundle": ">=5.4,<8",
        "twig/twig": "^3.0.0",
        "vimeo/psalm": "^6.13.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Sorter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sorter\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "doctrine/orm": "Needed for use of ORMApplier",
        "symfony/property-access": "Needed for use of ArrayAdapter"
    },
    "authors": [
        {
            "name": "Yohan Giarelli",
            "email": "yohan@les-tilleuls.coop"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
